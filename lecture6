#include <bits/stdc++.h>
using namespace std;

int square1(int n)
{

	// main()
	cout << "address of n1 in square1(): " << &n << "\n";

	n *= n;
	return n;
}

void square2(int* n)
{

	cout << "address of n2 in square2(): " << n << "\n";

	*n *= *n;
}

void square3(int& n)
{

	cout << "address of n3 in square3(): " << &n << "\n";

	n *= n;
}
